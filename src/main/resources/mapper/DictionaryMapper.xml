<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qulei.dao.DictionaryDao">

    <sql id="Base_column">
        DIC_ID,NAME,DESCRIPTION,VALUE_NUM,VALUE_STR,CREATE_TIME,UPDATE_TIME
    </sql>

    <!--查询字典内容（数字）-->
    <select id="getDictionaryNum" parameterType="String" resultType="int">
        SELECT value_num FROM DICTIONARY WHERE dic_id=#{dic_id}
    </select>

</mapper>